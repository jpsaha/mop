<td>{{ item.yr }}</td>

<!-- QN column -->
<td>
  {% if item[include.contest].yr %}
    {% for member in item[include.contest].qn %}
      {% if member.qq %}
        {{ member.src }}
        <a href="{{ member.qq }}" target="_blank" rel="noopener noreferrer">
          <i class="fa-solid fa-file-pdf fa-2x"></i>
        </a>
      {% endif %}
      {% if member.sol %}
        <a href="{{ member.sol }}" target="_blank" rel="noopener noreferrer">
          <i class="fa-solid fa-file-pdf fa-2x"></i>
        </a>
      {% endif %}
      {% unless forloop.last %}<br>{% endunless %}
    {% endfor %}
  {% endif %}
</td>

<!-- AoPS column -->
<td>
  {% if item[include.contest].aops %}
    {% for member in item[include.contest].aops %}
      {% if member.qq %}
        {{ member.src }}
        <a href="{{ member.qq }}" target="_blank" rel="noopener noreferrer">
          <i class="fa-solid fa-globe fa-2x"></i>
        </a>
      {% endif %}
      {% unless forloop.last %}<br>{% endunless %}
    {% endfor %}
  {% endif %}
</td>

<!-- Discussion / Rank column -->
<td>
  {% if item[include.contest].discussion %}
    <a href="{{ site.url }}{{ site.baseurl }}/assets/pdf/{{ tst | upcase }}/{{ item[include.contest].discussion }}.pdf"
       target="_blank" rel="noopener noreferrer">
      <i class="fa-solid fa-file-pdf fa-2x"></i>
    </a>
  {% endif %}

  {% if item[include.contest].rk %}
    {%- capture rk -%}{{ item[include.contest].rk }}{%- endcapture -%}
    {{ rk | markdownify }}
  {% endif %}
</td>
